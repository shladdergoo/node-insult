{
    "extension": [
        ".ts",
        ".tsx"
    ],
    "exclude": [
        "__tests__",
        "**/model",
        "**/types.ts",
        "**/*.config.ts",
        "**/*.d.ts"
    ],
    "reporter": [
        "cobertura",
        "html",
        "text",
        "text-summary"
    ],
    "temp-directory": "./coverage/.nyc_output",
    "include-all-sources": "true"
}
